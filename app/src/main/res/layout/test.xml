<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="test"
            type="String" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
